{
  "name": "react-elmish-utils",
  "version": "4.2.2",
  "description": "Utilities for react-elmish",
  "author": "atheck",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "7.16.0",
    "@babel/core": "7.16.0",
    "@babel/plugin-proposal-class-properties": "7.16.0",
    "@babel/plugin-transform-runtime": "7.16.4",
    "@babel/preset-env": "7.16.4",
    "@babel/preset-typescript": "7.16.0",
    "@types/jest": "27.0.3",
    "@types/react": "17.0.37",
    "eslint": "8.3.0",
    "eslint-config-heck": "1.1.2",
    "jest": "27.4.3",
    "ts-jest": "27.0.7",
    "tslib": "2.3.1",
    "typemoq": "2.1.0",
    "typescript": "4.5.2"
  },
  "peerDependencies": {
    "react-elmish": ">=1.0.0"
  },
  "scripts": {
    "install-peers": "npm install --no-save react-elmish react",
    "prebuild": "npm run install-peers",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly --project ./src",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline",
    "lint": "npx eslint --ext .ts,.tsx ./src",
    "pretest": "npm run install-peers",
    "test": "jest",
    "pretest:watch": "npm run install-peers",
    "test:watch": "jest --watch"
  },
  "homepage": "https://github.com/atheck/react-elmish-utils",
  "repository": {
    "type": "git",
    "url": "https://github.com/atheck/react-elmish-utils.git"
  },
  "keywords": [
    "react",
    "elmish",
    "utilities",
    "form",
    "validation"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts"
}
