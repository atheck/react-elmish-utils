{
  "name": "react-elmish-utils",
  "version": "0.2.0",
  "description": "Utilities for react-elmish",
  "author": "atheck",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "7.13.16",
    "@babel/core": "7.13.16",
    "@babel/plugin-proposal-class-properties": "7.13.0",
    "@babel/plugin-transform-runtime": "7.13.15",
    "@babel/preset-env": "7.13.15",
    "@babel/preset-typescript": "7.13.0",
    "@types/jest": "26.0.22",
    "@types/react": "17.0.3",
    "jest": "26.6.3",
    "ts-jest": "26.5.5",
    "tslib": "2.2.0",
    "typemoq": "2.1.0",
    "typescript": "4.2.4"
  },
  "peerDependencies": {
    "react-elmish": ">=1.0.0"
  },
  "scripts": {
    "install-peers": "npm install --no-save react-elmish react",
    "prebuild": "npm run install-peers",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline",
    "pretest": "npm run install-peers",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "homepage": "https://github.com/atheck/react-elmish-utils",
  "repository": {
    "type": "git",
    "url": "https://github.com/atheck/react-elmish-utils.git"
  },
  "keywords": [
    "react",
    "elmish",
    "utilities",
    "form",
    "validation"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts"
}
